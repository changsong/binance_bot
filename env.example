# Binance 模式: testnet 或 main
BINANCE_MODE=testnet

# Binance Testnet API 密钥
BINANCE_TEST_API_KEY=your_test_api_key_here
BINANCE_TEST_API_SECRET=your_test_api_secret_here

# Binance Mainnet API 密钥（生产环境使用）
BINANCE_MAIN_API_KEY=your_main_api_key_here
BINANCE_MAIN_API_SECRET=your_main_api_secret_here

# Webhook 安全密钥
WEBHOOK_SECRET=your_webhook_secret_here

# 飞书通知 Webhook（可选）
FEISHU_WEBHOOK=https://open.feishu.cn/open-apis/bot/v2/hook/your_webhook_url

# Flask 服务端口（可选，默认 80）
PORT=80

